{
  "name" : "parsingcorner/cron-manager-bundle",
  "description" : "A simple cron manager that allow to schedule commands without relying on system operators",
  "type" : "symfony-bundle",
  "authors" : [{
    "name" : "Parsing Corner",
    "email" : "parsingcorner@gmail.com"
  }],
  "keywords" : [
    "cron", "manager", "command", "task", "schedule"
  ],
  "license" : [
    "MIT"
  ],
  "require" : {
    "php": ">=5.3.9",
    "symfony/form": "~2.3|~3.0",
    "symfony/framework-bundle": "~2.3|~3.0",
    "symfony/twig-bundle": "~2.3|~3.0",
    "doctrine/doctrine-bundle": "~1.3",
    "doctrine/orm": "~2.5",
    "symfony/console": "~2.3|~3.0",
    "symfony/yaml": "~2.3|~3.0"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "~2.7|~3.0",
    "phpunit/phpunit": "4.7.*"
  },
  "autoload" : {
    "psr-0" : {
      "Parsingcorner\\CronManagerBundle" : ""
    }
  },
  "target-dir" : "Parsingcorner/CronManagerBundle"
}